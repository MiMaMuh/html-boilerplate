{
	"presets": [
		[
			"env",
			{
				"useBuiltIns": "entry",
				"ignoreBrowserslistConfig": false,
				"targets": {
					"node": 8,
					"browsers": [
						"last 3 versions",
						"> 1% in DE",
						"Chrome >= 50",
						"Firefox >= 44",
						"Safari >= 9",
						"iOS >= 9",
						"Explorer >= 10",
						"Edge >= 12"
					]
				}
			}
		],
		"stage-0",
		"react"
	],
	"plugins": [
		"transform-class-properties",
		"transform-es2015-classes",
		"add-module-exports"
	],
	"env": {
		"production": {},
		"development": {
			"plugins": [
				[
					"flow-runtime",
					{
						"assert": true,
						"annotate": true
					}
				]
			]
		}
	}
}
